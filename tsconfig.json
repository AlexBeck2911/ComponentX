{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "jsx": "preserve",
    "jsxImportSource": "@builder.io/mitosis",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "components"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src"
  ]
}